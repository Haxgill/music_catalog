<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minWidth="600.0" stylesheets="@TrackList.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.trackList.TrackListController">
   <children>
      <TableView fx:id="trackListTable" minWidth="600.0" nodeOrientation="LEFT_TO_RIGHT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="trackNameColumn" maxWidth="2500.0" minWidth="185.0" prefWidth="250.0" text="Название песни" />
          <TableColumn fx:id="artistNameColumn" maxWidth="2000.0" minWidth="148.0" prefWidth="200.0" text="Исполнитель" />
            <TableColumn fx:id="albumNameColumn" maxWidth="2000.0" minWidth="148.0" prefWidth="200.0" text="Альбом" />
            <TableColumn fx:id="albumYearColumn" maxWidth="900.0" minWidth="65.0" prefWidth="90.0" text="Год албома" />
            <TableColumn fx:id="trackTimeColumn" maxWidth="730.0" minWidth="54.0" prefWidth="73.0" text="Время" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
